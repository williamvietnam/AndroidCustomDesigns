<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolbarCustom">
        <attr name="startIconDraw" format="reference" />
        <attr name="toolbarTitle" format="string" />
        <attr name="toolbarTitleRes" format="reference" />
        <attr name="endIconDraw" format="reference" />
    </declare-styleable>

    <declare-styleable name="Toolbar2">

    </declare-styleable>

    <declare-styleable name="TextFieldCustom">
        <attr name="textHint" format="string" />
        <attr name="hintRes" format="integer" />
        <attr name="startIcon" format="reference" />
        <attr name="text" format="string" />
        <attr name="textRes" format="integer" />
        <attr name="imeOption" format="string" />
        <attr name="inputTypes" format="string" />
        <attr name="drawableEnd" format="reference" />
    </declare-styleable>

    <declare-styleable name="BottomNavigationView1">
        <attr name="tabSize" format="integer" />
        <attr name="iconTab1" format="integer|reference" />
        <attr name="iconTab2" format="integer|reference" />
        <attr name="iconTab3" format="integer|reference" />
        <attr name="iconTab4" format="integer|reference" />
        <attr name="iconTab5" format="integer|reference" />
        <attr name="titleTab1" format="integer|string" />
        <attr name="titleTab2" format="integer|string" />
        <attr name="titleTab3" format="integer|string" />
        <attr name="titleTab4" format="integer|string" />
        <attr name="titleTab5" format="integer|string" />
    </declare-styleable>

    <declare-styleable name="LoginScreen">
        <attr name="title" format="string" />
        <attr name="hintAccount" format="string|integer" />
        <attr name="hintPassword" format="string|integer" />
        <attr name="buttonName" format="integer|string" />
    </declare-styleable>

    <declare-styleable name="TagView">
        <attr name="vertical_spacing" format="dimension" />
        <attr name="horizontal_spacing" format="dimension" />
    </declare-styleable>

    <!--arc slider-->
    <declare-styleable name="SeekArc">
        <attr name="thumb" format="reference" />
        <attr name="thumbOffset" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="progressWidth" format="dimension" />
        <attr name="arcWidth" format="dimension" />
        <attr name="progress" format="integer" />
        <attr name="rotation" format="integer" />
        <attr name="startAngle" format="integer" />
        <attr name="sweepAngle" format="integer" />
        <attr name="arcColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="roundEdges" format="boolean" />
        <attr name="touchInside" format="boolean" />
        <attr name="clockwise" format="boolean" />
        <attr name="enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SeekArcTheme">
        <attr name="seekArcStyle" format="reference" />
    </declare-styleable>
</resources>